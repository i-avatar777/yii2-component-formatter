{
  "name": "i-avatar777/yii2-component-formatter",
  "version":"0.0.7",
  "description": "Компонент для форматирования расширяющий стандартный датами в русском календаре",
  "keywords": ["formatter"],
  "type": "project",
  "license": "BSD-3-Clause",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "i-avatar777/service-date-rus": "*",
    "yiisoft/yii2": "*"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-jui": "*",
    "yiisoft/yii2-faker": "*"
  },
  "autoload": {
    "psr-4": {
      "iAvatar777\\components\\FormatterRus\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "process-timeout": 1800
  }
}